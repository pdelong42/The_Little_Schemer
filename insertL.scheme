(define insertL
   (lambda
      (new old lat)
      (cond
         (  (null? lat)
            (quote ())
         )
         (  (eq? old (car lat))
            (cons new lat)
         )
         (else
            (cons
               (car lat)
               (insertL new old (cdr lat))
            )
         )
      )
   )
)

